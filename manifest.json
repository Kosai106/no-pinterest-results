{
  "name": "No Pinterest Results",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Removes any Pinterest results from your Google search queries.",
  "author": "Kevin Ã˜sterkilde",
  "omnibox": {
    "keyword": "np"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_title": "No Pinterest"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}